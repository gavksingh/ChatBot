{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" },
      { "src": "client/package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/socket.io/(.*)", "dest": "/server.js" },
      { "src": "/api/(.*)", "dest": "/server.js" },
      { "src": "/(.*)", "dest": "/client/build/$1" }
    ]
  }
  